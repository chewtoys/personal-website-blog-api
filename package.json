{
  "name": "blog-api",
  "version": "1.0.0",
  "engines": {
    "node": "6.11.1"
  },
  "description": "a simple blog api ",
  "main": "app.js",
  "directories": {
    "test": "tests"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    }
  },
  "scripts": {
    "postinstall": "[ \"$NODE_ENV\" != production ] && exit 0; npm run production",
    "production": "NODE_ENV=production npm run build",
    "build": "rimraf dist/ && babel ./src --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "start:prod": "node dist/bin/www",
    "start": "npm run start:prod",
    "start:dev": "NODE_ENV=development nodemon --watch src --exec babel-node -- src/bin/www",
    "re:migrate": "sequelize db:migrate:undo:all && sequelize db:migrate && sequelize db:seed:all",
    "seed:db": "sequelize db:seed:all",
    "test": "NODE_ENV=test npm run test:server",
    "test:server": "nyc --reporter=html --reporter=text mocha tests/server/tests.js --compilers js:babel-register",
    "seed:test:db": "NODE_ENV=test sequelize db:seed:all",
    "re:migrate:test": "NODE_ENV=test sequelize db:migrate:undo:all && NODE_ENV=test sequelize db:migrate",
    "eslint": "eslint --fix **/*.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com/jattoabdul/personal-website-blog-api.git"
  },
  "keywords": [
    "blog",
    "api",
    "personal website"
  ],
  "author": "Aminujatto Abdulqahhar <jattoade@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jattoabdul/amasafirdaus/issues"
  },
  "homepage": "https://github.com/jattoabdul/amasafirdaus#readme",
  "dependencies": {
    "axios": "^0.16.2",
    "babel": "^6.23.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-jest": "^21.0.2",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "babel-watch": "^2.0.6",
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "^3.5.1",
    "body-parser": "^1.17.2",
    "chai": "^4.0.2",
    "chai-fetch-mock": "^1.0.1",
    "chai-http": "^3.0.0",
    "concurrently": "^3.5.0",
    "cool-ascii-faces": "1.3.4",
    "crypto-js": "^3.1.9-1",
    "dotenv": "^4.0.0",
    "eslint": "^4.1.1",
    "expect": "^21.0.2",
    "express": "^4.15.3",
    "faker": "^4.1.0",
    "fetch-mock": "^5.13.1",
    "import-export": "^1.0.1",
    "istanbul": "^0.4.5",
    "jsdom": "^11.2.0",
    "jsonwebtoken": "^7.4.1",
    "jusibe": "^1.0.1",
    "lite-server": "^2.3.0",
    "lodash": "^4.17.4",
    "mocha": "^3.4.2",
    "mocha-istanbul": "^0.3.0",
    "mocha-lcov-reporter": "^1.3.0",
    "mocha-sinon": "^2.0.0",
    "moment": "^2.18.1",
    "morgan": "^1.8.2",
    "nexmo": "^2.0.2",
    "nock": "^9.0.14",
    "node-datetime": "^1.0.1",
    "node-sass": "^4.5.3",
    "nodemailer": "^4.0.1",
    "nodemon": "^1.11.0",
    "npm": "^5.3.0",
    "nyc": "^11.0.3",
    "pg": "^6.4.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.3.1",
    "sequelize-cli": "^2.7.0",
    "sequelize-mocking": "^0.4.1",
    "shortid": "^2.2.8",
    "sinon": "^2.3.6",
    "socket.io": "^2.0.3",
    "supertest": "^3.0.0",
    "uglify-js": "^2.8.23",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "babel-plugin-transform-runtime": "^6.23.0",
    "coveralls": "^2.13.1",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "jshint": "^2.9.5",
    "mock-local-storage": "^1.0.5"
  }
}
